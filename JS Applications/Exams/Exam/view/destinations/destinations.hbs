       {{> header}}
        <section id="viewMydestinations">
            <h3>My destinations</h3>
           
           
            <div class="destination-ticket">
                <div class="destination-left">
                    <img src="{{imageURL}}"
                        alt="">
                </div>
                <div class="destination-right">
                    <div>
                        <h3>{{destination}}</h3><span>{{dateDeparture}}</span>
                    </div>
                    <div>
                        {{city}}
                    </div>
                    <p>{{destination.duration}}</p>
                    <a href="#/close/{{destination.id}}" class="remove">REMOVE</a>
                    <a href="#/details/{{destination.id}}" class="details">Details</a>
                </div>
            </div>
           
          
        </section>
        {{> footer}}